    {
        "id": "381359c124a3d83e",
        "type": "function",
        "z": "61f2634f4e3380cc",
        "name": "Parse Metrics",
        "func": "// ‚úÖ CORRECTION: Validation des donn√©es\nif (!msg.payload || typeof msg.payload !== 'object') {\n    node.warn(\"Metrics invalide: payload manquant ou incorrect\");\n    return null;\n}\n\nconst data = msg.payload;\n\ntry {\n    msg.payload = {\n        uptime: data.uptime || 0,\n        samples: data.total_samples || 0,\n        inferences: data.total_inferences || 0,\n        seizures: data.seizures_detected || 0,\n        free_heap: data.free_heap || 0,\n        wifi_rssi: data.wifi_rssi || 0,\n        // ‚úÖ CORRECTION: Utiliser la vraie valeur au lieu de hardcoder\n        bt_connected: data.bt_connected !== undefined ? data.bt_connected : true,\n        seizure_active: data.seizure_active || false\n    };\n    \n    return msg;\n} catch (error) {\n    node.error(\"Erreur parsing metrics: \" + error.message);\n    return null;\n}",
