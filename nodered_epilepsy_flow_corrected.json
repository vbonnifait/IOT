    {
        "id": "e72144ca86145b0f",
        "type": "function",
        "z": "61f2634f4e3380cc",
        "name": "Parse Alert",
        "func": "// ‚úÖ CORRECTION: Validation des donn√©es\nif (!msg.payload || typeof msg.payload !== 'object') {\n    node.warn(\"Alert invalide: payload manquant ou incorrect\");\n    return null;\n}\n\nconst data = msg.payload;\n\n// V√©rifier que alert_type existe\nif (!data.alert_type) {\n    node.warn(\"Alert invalide: champ 'alert_type' manquant\");\n    return null;\n}\n\ntry {\n    if (data.alert_type === \"SEIZURE_START\") {\n        msg.payload = {\n            type: \"warning\",\n            title: \"‚ö†Ô∏è ALERTE CRISE D√âTECT√âE\",\n            message: \"Crise √©pileptique d√©tect√©e par le syst√®me\",\n            timestamp: new Date(data.timestamp)\n        };\n        return msg;\n    } else if (data.alert_type === \"SEIZURE_END\") {\n        msg.payload = {\n            type: \"info\",\n            title: \"‚úì Fin de crise\",\n            message: `Dur√©e: ${data.duration || 'N/A'}s`,\n            timestamp: new Date(data.timestamp)\n        };\n        return msg;\n    } else {\n        // ‚úÖ CORRECTION: G√©rer les types d'alerte inconnus\n        node.warn(\"Type d'alerte inconnu: \" + data.alert_type);\n        return null;\n    }\n} catch (error) {\n    node.error(\"Erreur parsing alert: \" + error.message);\n    return null;\n}",
