    {
        "id": "cc8bfa09fd958b33",
        "type": "function",
        "z": "61f2634f4e3380cc",
        "name": "Parse Prediction",
        "func": "// ‚úÖ CORRECTION: Validation des donn√©es\nif (!msg.payload || typeof msg.payload !== 'object') {\n    node.warn(\"Prediction invalide: payload manquant ou incorrect\");\n    return null;\n}\n\nconst data = msg.payload;\n\n// V√©rifier que confidence existe\nif (data.confidence === undefined || data.confidence === null) {\n    node.warn(\"Prediction invalide: champ 'confidence' manquant\");\n    return null;\n}\n\ntry {\n    // ‚úÖ CORRECTION: Multiplier par 100 pour convertir 0-1 en 0-100%\n    // Arrondir √† 1 d√©cimale\n    msg.payload = Math.round(data.confidence * 100 * 10) / 10;\n    \n    // Assurer que la valeur est dans la plage 0-100\n    msg.payload = Math.max(0, Math.min(100, msg.payload));\n    \n    return msg;\n} catch (error) {\n    node.error(\"Erreur parsing prediction: \" + error.message);\n    return null;\n}",
        "outputs": 1,
        "timeout": "",
        "noerr": 0,
        "initialize": "",
