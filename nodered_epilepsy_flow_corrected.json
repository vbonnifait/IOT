    {
        "id": "46d3c1174bab5577",
        "type": "function",
        "z": "61f2634f4e3380cc",
        "name": "Parse Signal EEG",
        "func": "// ‚úÖ CORRECTION: Validation des donn√©es\nif (!msg.payload || typeof msg.payload !== 'object') {\n    node.warn(\"Signal EEG invalide: payload manquant ou incorrect\");\n    return null;\n}\n\nconst data = msg.payload;\n\n// V√©rifier que les champs requis existent\nif (data.microvolts === undefined || data.microvolts === null) {\n    node.warn(\"Signal EEG invalide: champ 'microvolts' manquant\");\n    return null;\n}\n\n// Envoyer uniquement la valeur EEG en microvolts au chart\ntry {\n    msg.payload = Number(data.microvolts.toFixed(2)); // arrondi √† 2 d√©cimales\n    msg.topic = \"EEG (¬µV)\";\n    return msg;\n} catch (error) {\n    node.error(\"Erreur parsing EEG: \" + error.message);\n    return null;\n}",
        "outputs": 1,
        "timeout": "",
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
