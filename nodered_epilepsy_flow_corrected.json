    {
        "id": "9289de70fdf058d9",
        "type": "function",
        "z": "61f2634f4e3380cc",
        "name": "Parse Status",
        "func": "// ‚úÖ CORRECTION: Validation des donn√©es\nif (!msg.payload || typeof msg.payload !== 'object') {\n    node.warn(\"Status invalide: payload manquant ou incorrect\");\n    return null;\n}\n\nconst data = msg.payload;\n\n// V√©rifier que state existe\nif (!data.state) {\n    node.warn(\"Status invalide: champ 'state' manquant\");\n    return null;\n}\n\ntry {\n    let statusText = \"\";\n    let statusColor = \"\";\n    \n    // ‚úÖ CORRECTION: G√©rer plus d'√©tats possibles\n    switch(data.state) {\n        case \"online\":\n            statusText = \"üü¢ Syst√®me en ligne\";\n            statusColor = \"green\";\n            break;\n        case \"ready\":\n            statusText = \"üü¢ Syst√®me pr√™t\";\n            statusColor = \"green\";\n            break;\n        case \"initializing\":\n            statusText = \"üü° Initialisation...\";\n            statusColor = \"orange\";\n            break;\n        case \"error\":\n            statusText = \"üî¥ Erreur syst√®me\";\n            statusColor = \"red\";\n            break;\n        case \"offline\":\n            statusText = \"‚ö´ Syst√®me hors ligne\";\n            statusColor = \"gray\";\n            break;\n        case \"timeout\":\n            statusText = \"üü† Timeout d√©tect√©\";\n            statusColor = \"orange\";\n            break;\n        default:\n            statusText = \"‚ö™ \" + data.state;\n            statusColor = \"gray\";\n            node.warn(\"√âtat syst√®me inconnu: \" + data.state);\n    }\n    \n    msg.payload = statusText;\n    msg.color = statusColor;\n    \n    return msg;\n} catch (error) {\n    node.error(\"Erreur parsing status: \" + error.message);\n    return null;\n}",
